{
   "bindings" : {
      "/actions/donut" : {
         "poses" : [
            {
               "output" : "/actions/donut/in/Hand_Left",
               "path" : "/user/hand/left/pose/raw"
            },
            {
               "output" : "/actions/donut/in/Hand_Right",
               "path" : "/user/hand/right/pose/raw"
            }
         ],
         "haptics" : [
            {
               "output" : "/actions/donut/out/haptic_right",
               "path" : "/user/hand/right/output/haptic"
            },
            {
               "output" : "/actions/donut/out/haptic_left",
               "path" : "/user/hand/left/output/haptic"
            }
         ],
        "sources": [
          {
            "inputs": {
              "click": {
                "output": "/actions/donut/in/LeftButton"
              }
            },
            "mode": "button",
            "path": "/user/hand/left/input/trigger"
          },
          {
            "inputs": {
              "click": {
                "output": "/actions/donut/in/RightButton"
              }
            },
            "mode": "button",
            "path": "/user/hand/right/input/trigger"
          }
        ]
      }
   },
   "controller_type" : "vive_controller",
   "description" : "Bindings for Donut Viewer for the Vive controller",
   "name" : "Donut Viewer bindings for Vive Controller"
}
